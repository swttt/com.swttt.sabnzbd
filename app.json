{
    "id": "com.swttt.sabnzbd",
    "name": {
        "en": "sabNZBd",
        "nl": "sabNZBd"
    },
    "description": {
        "en": "sabNZBd app for Homey, to control sab using flows and view statistics.",
        "nl": "sabNZBd app voor Homey, bestuur sab d.m.v. flows en bekijk download snelheid in inzichten."
    },
    "category": "internet",
    "images":{
        "large":"./assets/images/large.jpg",
        "small":"./assets/images/small.jpg"
    },
    "version": "0.0.1",
    "compatibility": "0.x",
    "author": {
        "name": "Bas Jansen",
        "email": "bas@balotti.com"
    },
    "flow": {
      "actions": [
        {
          "id": "pause_sabnzbd",
                "title": {
                    "en": "Pause downloads",
                    "nl": "Pauzeer downloads"
                },
                "args": [
                    {
                        "name": "device",
                        "type": "device",
                        "filter": "driver_id=sabnzbd"
                    }
                ]
              },
              {
                "id": "resume_sabnzbd",
                      "title": {
                          "en": "Resume downloads",
                          "nl": "Hervat downloads"
                      },
                      "args": [
                          {
                              "name": "device",
                              "type": "device",
                              "filter": "driver_id=sabnzbd"
                          }
                      ]
                    }
            ]
          },
    "dependencies": {
      "request": "*"
    },
    "drivers": [
        {
            "id": "sabnzbd",
            "name": {
                "en": "sabNZBd",
                "nl": "sabNZBd"
            },
            "images": {
                "large": "drivers/sabnzbd/assets/images/large.jpg",
                "small": "drivers/sabnzbd/assets/images/small.jpg"
            },
            "class": "other",
            "capabilities": [ "download_speed" ],
            "pair": [
                {
                    "id": "list_sabnzbd"

                }
            ]
        }
    ]

}
